{
  "name": "@cloudwalk/chainshot",
  "version": "0.4.0",
  "description": "",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc -p tsconfig.json && cp -r src/templates dist/",
    "build:watch": "tsc -p tsconfig.json --watch",
    "prepare": "rm -rf dist && npm run build"
  },
  "dependencies": {
    "handlebars": "^4.7.8",
    "mocha-chai-jest-snapshot": "^1.1.7"
  },
  "devDependencies": {
    "@cloudwalk/eslint-config-smart-contracts": "^1.0.0",
    "@nomicfoundation/hardhat-ethers": "^3.0.0",
    "hardhat": "^2.0.0",
    "@types/mocha": "^10.0.0",
    "@types/chai": "^4.0.0",
    "@types/node": "^22.0.0",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "@nomicfoundation/hardhat-ethers": "^3.0.0",
    "hardhat": "^2.0.0",
    "mocha": ">=10",
    "chai": ">=4"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./hook": {
      "require": "./dist/hook.js",
      "types": "./dist/hook.d.ts"
    }
  },
  "files": [
    "dist/"
  ],
  "publishConfig": {
    "access": "public"
  }
